{
  "name": "@rickzhou/editor",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "React Editor",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx,json}",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx,json}'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "theme": "node scripts/gen-editor-theme.mjs"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.5.1",
    "@rickzhou/react-css": "^2.1.1",
    "@rickzhou/react-utils": "^2.0.1",
    "antd": ">=5.6.1",
    "color": "^4.2.3",
    "file-saver": "^2.0.5",
    "lodash": "^4.17.21",
    "monaco-editor": "^0.39.0",
    "react-contexify": "^6.0.0",
    "react-split-pane": "^0.1.92",
    "vscode-material-icon-theme-js": "^1.0.7"
  },
  "devDependencies": {
    "@rickzhou/react-fabric": "^4.0.0",
    "@types/color": "^3.0.3",
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@vitejs/plugin-react": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-pwa": "^0.16.4"
  },
  "peerDependencies": {
    "antd": ">=5.4.2",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "overrides": {
    "react-split-pane": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  }
}
